   "source": [
    "def find_sum_three(inputs, sum_to):\n",
    "    while inputs:\n",
    "        x=inputs.pop()\n",
    "        y=find_sum(inputs.copy(), sum_to-x)\n",
    "        if y:\n",
    "            return x*y\n",
    "        \n",
    "print(find_sum_three(inputs.copy(), 2020))"
   ]
